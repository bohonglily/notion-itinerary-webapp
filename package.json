{
  "name": "notion-itinerary-webapp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "dev:netlify": "concurrently \"npx vite --port 5173\" \"npx netlify functions:serve --port 8888\"",
    "dev:vercel": "vercel dev --listen 3000",
    "dev:netlify:alt": "concurrently \"npx vite --port 5174\" \"npx netlify functions:serve --port 8889\"",
    "build": "vite build",
    "build:netlify": "npm run build",
    "build:vercel": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:vercel": "vercel --prod",
    "platform:detect": "echo 'Platform detection: Currently manual - check domain for auto-detection'",
    "platform:info": "echo 'Platform info: Netlify (.netlify.app) or Vercel (.vercel.app) auto-detected at runtime'"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-cli": "^22.2.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vercel": "^37.0.0",
    "vite": "^5.4.2"
  }
}
