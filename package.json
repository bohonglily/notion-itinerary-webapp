{
  "name": "notion-itinerary-webapp",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "dev:vite": "vite --port 5173",
    "dev:netlify": "netlify dev",
    "dev:vercel": "vercel dev --listen 3000",
    "dev:vercel-alt": "concurrently \"vite --port 5173\" \"vercel dev --listen 3001\"",
    "build": "vite build",
    "build:netlify": "npm run build",
    "build:vercel": "npm run build",
    "build:staging": "cross-env NODE_ENV=staging npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:staging": "vercel --target staging",
    "deploy:vercel:production": "vercel --prod --target production",
    "deploy:staging": "vercel deploy --prebuilt --target staging",
    "deploy:production": "vercel deploy --prebuilt --prod",
    "netlify:dev": "netlify dev",
    "clean:ports": "taskkill /F /IM node.exe 2>nul || echo \"Ports cleaned\"",
    "vercel:clean-start": "npm run clean:ports && npm run dev:vercel",
    "env:check": "node -p \"'NODE_ENV: ' + process.env.NODE_ENV + ', VITE_ENV: ' + process.env.VITE_ENV\"",
    "env:verify": "node scripts/verify-environment.js",
    "env:verify:staging": "cross-env NODE_ENV=staging node scripts/verify-environment.js staging",
    "env:verify:production": "cross-env NODE_ENV=production node scripts/verify-environment.js production",
    "env:staging": "cross-env NODE_ENV=staging VITE_ENV=staging",
    "env:production": "cross-env NODE_ENV=production VITE_ENV=production",
    "prebuild:staging": "npm run env:verify:staging",
    "prebuild:production": "npm run env:verify:production",
    "setup:vercel": "node scripts/setup-vercel.js",
    "setup:env": "node scripts/verify-environment.js"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-cli": "^22.2.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vercel": "^37.0.0",
    "vite": "^5.4.2"
  }
}
